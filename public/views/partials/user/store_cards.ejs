<div id="cardsTiendas" class="">
    <% tiendas.forEach(function(tienda) { %>
        <div class="card mb-3 mt-4">
            <div class="row card-body align-items-center">
                <div class="col-md-3">
                    <img src="https://cdn1.iconfinder.com/data/icons/farm-flat/64/stall-store-market-512.png" class="img-fluid rounded mx-auto d-block" alt="Market" style="max-width:60%">
                </div>
                <div class="col-md-9">
                    <div class="card-body">
                        <h5 class="card-title"><%= tienda.nombreTienda %></h5>
                        <p class="card-text"><small class="text-muted">1hr o programada</small></p>
                        <a class="btn btn-success" role="button" href="<%= tienda.id %>/productos">Ir a la tienda</a>
                    </div>
                </div>
            </div>
        </div>
    <% }) ;%>
</div>